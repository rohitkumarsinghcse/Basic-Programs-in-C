/*Minimum number in series of numbers*/
#include<stdio.h>
int main()
{
	int a[50],i,n,min;
	printf("Ener the size of array :\n");
	scanf("%d",&n);
	printf("Enter the array elements:\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
		
	}
	min=a[0];
	for(i=1;i<n;i++)
	{
		if(a[i]<min)
		min=a[i];
		
	}
	printf("Minimum number is %d ",min);
	return 0;
	
}
